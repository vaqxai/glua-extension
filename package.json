{
  "name": "glua-extension",
  "displayName": "GLua Extension",
  "description": "Extending GLua with more grammars",
  "version": "1.0.3",
  "publisher": "Vaqxai",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
